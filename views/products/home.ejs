<% layout("./layouts/boilerplate") -%>

<body>
    <h1>All Products</h1>
    <form action="/products/new">
        <button>Create new product</button>
    </form>
    <% products.forEach((product)=>{ %>
        <li>
            <a href="/products/<%= product._id %>">
                <%= product.name %> <br>
            </a>
        </li>
        <% }); %>
</body>
